<div class="container">
	<div class="present">
		<div class="present-text">
			<h3>Find your inspiration.</h3>
			<div class="present-content">Join the Bijishashi community.
			</div>
			<% if user_signed_in? %>
			<% @image.each do |image| %>
			<br><br><br>
			<div class="abou<!-- t-us" id="about-path">
				<div class="wrapper" id="wrapper1">
					<div class="image-wrapper">
						<%= image_tag(image.file, alt: "Picture", title: "Picture") %>
					</div>
					<div class="section-content">
						<div class="content-wrapper">
							<div class="content-title">
								<h3><%= image.title %></h3>
							</div>
							<div class="content-main">
							<%= image.user_id %>
							</div>
						</div>
					</div>
				</div>
			</div>
			<hr>
			<% end %>
			<% else %>
			<a href="/users/sign_up" class="join-btn">Sign Up Now</a>
			<% end  %>
		</div>
	</div>
</div>